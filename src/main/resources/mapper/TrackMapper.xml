<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.eyes.eyesspace.persistent.mapper.TrackMapper">
  <select id="getTitleNum" resultType="java.lang.Long">
    select count(*)
    from log_track
    <where>
        title = #{title}
      <if test="start!=null and start!=''">
        and create_time &gt; #{start}
      </if>
      <if test="end!=null and end!=''">
        and create_time &lt; #{end}
      </if>
    </where>
  </select>

  <select id="getFieldDistinctNum" resultType="java.lang.Long">
    select count(distinct ${field})
    from log_track
    <where>
      <if test="start!=null">
        create_time &gt; #{start}
      </if>
      <if test="end!=null">
        and create_time &lt; #{end}
      </if>
    </where>
  </select>
</mapper>